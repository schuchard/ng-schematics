{
  "name": "@schuchard/schematics-core",
  "version": "0.5.0",
  "description": "Schematic Utilities",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test:unit": "ava"
  },
  "keywords": [
    "schematics",
    "schematic utilities"
  ],
  "author": "Kevin Schuchard",
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/core": "12.2.18",
    "@angular-devkit/schematics": "12.2.18",
    "@phenomnomnominal/tsquery": "5.0.0",
    "@schematics/angular": "12.2.18",
    "fuzzy": "^0.1.3",
    "inquirer": "^9.1.4",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "inquirer-fuzzy-path": "^2.3.0",
    "lodash": "4.17.21",
    "rxjs": "^6.6.7",
    "strip-json-comments": "^5.0.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "ava": {
    "files": [
      "src/**/*.spec.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/schuchard/ng-schematics/tree/master/packages/schematics-core",
    "directory": "packages/@schuchard/schematics-core"
  }
}
