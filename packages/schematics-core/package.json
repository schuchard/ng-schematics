{
  "name": "@schuchard/schematics-core",
  "version": "0.4.1",
  "description": "Schematic Utilities",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test:unit": "ava"
  },
  "keywords": [
    "schematics",
    "schematic utilities"
  ],
  "author": "Kevin Schuchard",
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/core": "8.3.20",
    "@angular-devkit/schematics": "8.3.20",
    "@phenomnomnominal/tsquery": "4.0.0",
    "@schematics/angular": "8.3.20",
    "fuzzy": "0.1.3",
    "inquirer": "7.0.3",
    "inquirer-autocomplete-prompt": "1.0.1",
    "inquirer-fuzzy-path": "2.2.0",
    "lodash": "4.17.19",
    "rxjs": "6.4.0",
    "strip-json-comments": "3.0.1",
    "typescript": "~3.7.2"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "ava": {
    "files": [
      "src/**/*.spec.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/inquirer": "6.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/schuchard/ng-schematics/tree/master/packages/schematics-core",
    "directory": "packages/@schuchard/schematics-core"
  }
}
