{
  "name": "@schuchard/schematics-core",
  "version": "0.2.0",
  "description": "Schematic Utilities",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test:unit": "ava"
  },
  "keywords": [
    "schematics",
    "schematic utilities"
  ],
  "author": "Kevin Schuchard",
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/core": "8.3.20",
    "@angular-devkit/schematics": "8.3.20",
    "lodash": "4.17.15",
    "rxjs": "6.4.0",
    "typescript": "~3.7.2"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "ava": {
    "files": [
      "src/**/*.spec.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
