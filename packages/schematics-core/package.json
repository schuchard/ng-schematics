{
  "name": "@schuchard/schematics-core",
  "version": "0.1.0",
  "description": "Schematic Utilities",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test:ci": "ava"
  },
  "keywords": [
    "schematics",
    "schematic utilities"
  ],
  "author": "Kevin Schuchard",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.15"
  },
  "devDependencies": {
    "@types/lodash": "4.14.149"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "ava": {
    "files": [
      "src/**/*.spec.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
